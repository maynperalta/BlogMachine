<ul id="BlogList">
  {{#each blog }}
    <li class='list-group-item blog-item'>
      <a href="/blog/{{ id }}" class="blogTitle">{{title}}</a><br>
      <a class="blogBody">{{ body }}</a>
      Author: <a href="/member/{{ UserId }}">{{ UserName }}</a><br>
      Category: <a>{{ category }}</a><br>
      <a>Mood: {{ mood }}</a></li>
  {{/each}}
</ul>
